<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.mondo.collaboration.online.rap.widgets.ModelExplorer"
            icon="icons/bricks.png"
            id="org.mondo.collaboration.online.rap.widgets.ModelExplorer"
            name="Model Explorer"
            restorable="true">
      </view>
      <view
            class="org.mondo.collaboration.online.rap.widgets.CurrentUserView"
            id="org.mondo.collaboration.online.rap.widgets.CurrentUserView"
            name="Curent User"
            restorable="true">
      </view>
      <view
            class="org.mondo.collaboration.online.rap.widgets.ModelLogView"
            id="org.mondo.collaboration.online.rap.widgets.ModelLogView"
            name="Model Log"
            restorable="true">
      </view>
      <view
            class="org.mondo.collaboration.online.rap.widgets.WhiteboardChatView"
            id="org.mondo.collaboration.online.rap.widgets.WhiteboardChatView"
            name="Whiteboard Chat">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.mondo.collaboration.online.rap.widgets.MONDOTextEditor"
            default="false"
            extensions="macl,mpbl,eiq"
            id="org.mondo.collaboration.online.rap.widgets.mondotexteditor"
            name="MONDO Text Editor">
      </editor>
   </extension>

</plugin>
