user Alice


policy permitAttribute permit-overrides {

 	rule denyInputs deny W to Alice{
		query "org.mondo.collaboration.security.lens.test.algorithm.objectInput"
	}
	
	rule permitInputReferences permit R to Alice{
		query "org.mondo.collaboration.security.lens.test.algorithm.referenceinput"
	}
	
	rule denyInputnames deny R to Alice{
		query "org.mondo.collaboration.security.lens.test.algorithm.attributename"
	}
	
}