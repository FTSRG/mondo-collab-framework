package org.mondo.collaboration.security.lens.test.algorithm

import "http://www.ikerlan.es/domains/WTSpec4M/benchmark"
import "http://www.eclipse.org/emf/2002/Ecore"

pattern objectInput (obj : SystemInput){
	SystemInput (obj);
}

pattern objectOutput (obj : SystemOutput){
	SystemOutput (obj);
}

pattern attributename (obj : SystemInput, name){
	SystemInput.name (obj,name);
}

pattern attributeid (obj : SystemOutput, id){
	SystemOutput.id (obj,id);
}

pattern referenceoutput(ctrl : WTCtrl,Output : SystemOutput) {
	WTCtrl.output(ctrl, Output);
}

pattern referenceinput(ctrl : WTCtrl,Input : SystemInput) {
	WTCtrl.input(ctrl, Input);
}
